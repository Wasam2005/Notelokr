{
  "expo": {
    "name": "NoteLokr",
    "slug": "notelokr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "notelokr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
       "secure": true,
      "package": "com.wasam2005.notelokr",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-secure-store",
      [
        "expo-navigation-bar",
        {
          "backgroundColor": "#0a0a0a",
          "barStyle": "light"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "expo-navigation-bar": {
        "backgroundColor": "#0a0a0a",
        "barStyle": "light"
      },
      "eas": {
        "projectId": "6182f006-85be-4099-ac78-4424c92ea58a"
      }
    }
  }
}
